        public String otherNum;
    }


    public class CommonDto {

        @ApiModelProperty("是否开展")
        public String ifHave;
        @ApiModelProperty("手术量")
        public String operaCount;
        @ApiModelProperty("其中，急诊手术量")
        public String emergencyCount;

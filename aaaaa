    public class DsaDto {

        @ApiModelProperty("品牌、型号")
        public String dsaName;
        @ApiModelProperty("安装年月（例：2018.4）")
        public String dsaDate;
    }


    public class GroupDto {

        @ApiModelProperty("组内共多少人")
        public String totalNum;
        @ApiModelProperty("组内博士多少人")
        public String doctorNum;
        @ApiModelProperty("组内硕士多少人")
        public String masterNum;
        @ApiModelProperty("高级职称多少人")
        public String seniorNum;
        @ApiModelProperty("中级职称多少人")
        public String mediumNum;

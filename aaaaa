        public String sickKindRate;
        @ApiModelProperty("平均住院日")
        public String averageInHospital;
        @ApiModelProperty("平均手术费用")
        public String averageOperaCost;
        @ApiModelProperty("治愈率")
        public String cureRate;
        @ApiModelProperty("死亡率")
        public String diedRate;

